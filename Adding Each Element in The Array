import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int  n=sc.nextInt();
		int Array[]=new int[n];
		int newA[]=new int[n];
		int i=0,temp=0,temp1=0;
		for (i=0;i<n ;i++){
		     Array[i]=sc.nextInt();
		}
		for ( i=0;i<n ;i++){
		    temp=Array[i]%10;
		    temp1=Array[i]/10;
		    newA[i]= temp1+temp;
		    System.out.print(newA[i]+" ");
		}
		
	}
}
