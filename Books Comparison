import java.util.*;

public class Main {

    public static void main(String[] args) {
        // write your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int count=0;
        for(int i=1;i<n;i++){
            for(int j=1;j<n;j++){
                count++;
                System.out.println(i+" "+j);
            }
        }
        System.out.print(count);
    }
}
