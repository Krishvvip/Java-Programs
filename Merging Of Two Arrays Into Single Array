import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n+n];
		int arr1[]=new int[n];
		for(int i=0;i<n;i++){
		    arr[i]=sc.nextInt();
		}
		for(int i=0;i<n;i++){
		    arr1[i]=sc.nextInt();
		}
		for(int i=0;i<n;i++){ 
		    arr[n+i]=arr1[i];
		}
		for(int i:arr){
		   System.out.print(i+" "); 
		}		
	}
}
