import java.util.*;

public class Main
{   
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a=1,rem,mul=0;
        while(n>0){
            rem=n%2;
            mul=mul+(rem*a);
            n=n/2;
            a=a*10;
        }
        System.out.print(mul);
}
}
